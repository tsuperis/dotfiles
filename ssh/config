Host github
	User git
	HostName github.com
	TCPKeepAlive yes
	identitiesonly yes
	identityFile ~/.ssh/id_rsa

Host gitlab
	User git
	HostName gitlab.com
	TCPKeepAlive yes
	identitiesonly yes
	identityFile ~/.ssh/id_rsa

